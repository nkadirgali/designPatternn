<VBox minHeight="120.0" prefHeight="120.0" prefWidth="242.0" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.UserPlaneDecorationController">
   <children>
      <HBox prefHeight="45.0" prefWidth="242.0" spacing="10.0">
         <children>
            <VBox prefHeight="82.0" prefWidth="273.0" spacing="10.0">
               <children>
                  <Label text="BodyType:">
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="31.0" prefWidth="134.0" spacing="10.0">
                     <children>
                        <Button fx:id="biplane" mnemonicParsing="false" onAction="#setPlaneBodyType" prefHeight="23.0" prefWidth="72.0" text="Biplane">
                           <font>
                              <Font size="10.0" />
                           </font>
                        </Button>
                        <Button fx:id="jet" mnemonicParsing="false" onAction="#setPlaneBodyType" prefHeight="23.0" prefWidth="42.0" text="JET">
                           <font>
                              <Font size="10.0" />
                           </font>
                        </Button>
                        <Button fx:id="propeller" mnemonicParsing="false" onAction="#setPlaneBodyType" prefHeight="23.0" prefWidth="62.0" text="Propeller">
