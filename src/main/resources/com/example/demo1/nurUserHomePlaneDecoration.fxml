                        <Insets left="10.0" />
                     </padding>
                  </Label>
                  <HBox prefHeight="31.0" prefWidth="134.0" spacing="10.0">
                     <children>
                        <Button fx:id="businessBut" mnemonicParsing="false" onAction="#setPlaneClassType" prefHeight="23.0" prefWidth="57.0" text="Business">
