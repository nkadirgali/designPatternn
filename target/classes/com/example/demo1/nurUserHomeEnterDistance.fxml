<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="enterDistance" prefHeight="400.0" prefWidth="300.0" styleClass="userHomeFirstHalf" stylesheets="@css/adminScene.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.UserEnterDistanceController">
   <children>
      <Label text="Enter distance" translateX="44.0" translateY="10.0">
         <font>
            <Font size="26.0" />
         </font>
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </Label>
      <ChoiceBox fx:id="city1" prefWidth="150.0" translateX="50.0">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </ChoiceBox>
      <ChoiceBox fx:id="city2" prefWidth="150.0" translateX="50.0">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin></ChoiceBox>
      <Button fx:id="submitButton" alignment="CENTER" mnemonicParsing="false" onAction="#submitButtonOnAction" prefHeight="30.0" prefWidth="150.0" text="Let's Go!" translateX="50.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Label id="labelWarning1" fx:id="warning" stylesheets="@css/adminScene.css" translateX="50.0" translateY="10.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets left="25.0" right="25.0" />
   </padding>
</VBox>
